{
  "expo": {
    "name": "dare2fit-companion-app",
    "slug": "dare2fit-companion-app",
    "description": "This is a companion app to the dare2fit web platform. Use it to log you activity and calorie intake and check your notifications.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/dare2fit-05-circle.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/dare2fit-05.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A202C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/dare2fit-05-circle.png",
        "backgroundColor": "#1A202C"
      }
    },
    "web": {
      "favicon": "./src/assets/dare2fit-05-circle.png"
    }
  }
}
